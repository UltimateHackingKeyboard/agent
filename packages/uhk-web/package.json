{
  "name": "uhk-web",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./node_modules ./.angular",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "run-s -sn build:renderer",
    "build:web": "ng build --prod --aot",
    "build:renderer": "ng build --prod --aot --project=uhk-renderer",
    "server:renderer": "ng build --project=uhk-renderer --watch",
    "test-skip": "ng test --watch false",
    "lint": "run-p -snc lint:*",
    "lint:ng": "ng lint",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "ng e2e"
  },
  "private": true,
  "license": "See in LICENSE",
