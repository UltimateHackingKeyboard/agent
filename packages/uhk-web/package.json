{
  "name": "uhk-web",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "run-s -sn build:*",
    "build:web": "ng build --prod --aot",
    "build:renderer": "ng build --prod --aot --project=uhk-renderer",
    "server:renderer": "ng build --project=uhk-renderer --watch --source-map",
    "test-skip": "ng test --watch false",
    "lint": "run-p -snc lint:*",
    "lint:ng": "ng lint",
    "lint:renderer": "tslint --project src/tsconfig.renderer.json",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "ng e2e"
  },
  "private": true,
  "license": "See in LICENSE",
  "devDependencies": {
    "@angular/animations": "9.1.11",
    "@angular-builders/custom-webpack": "9.2.0",
    "@angular/cli": "9.1.10",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/compiler-cli": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/localize": "9.1.11",
    "@angular-devkit/build-angular": "0.901.10",
    "@angular-devkit/build-optimizer": "0.901.10",
    "@angular-devkit/core": "9.1.10",
    "@angular/forms": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@ngtools/webpack": "9.1.10",
    "@ngrx/effects": "8.2.0",
    "@ngrx/router-store": "8.2.0",
    "@ngrx/store": "8.2.0",
    "@ngrx/store-devtools": "8.2.0",
    "@types/node": "12.12.29",
    "angular-confirmation-popover": "4.2.2",
    "angular-notifier": "6.0.1",
    "bootstrap": "3.4.1",
    "codelyzer": "5.2.1",
    "dragula": "3.7.2",
