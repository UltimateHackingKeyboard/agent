{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "target": "ES2021",
    "types": ["node"],
    "lib": [
      "dom"
    ]
  },
  "paths": {
    "@angular/*": ["../node_modules/@angular/*"],
    "rxjs/*": ["../node_modules/rxjs/*"]
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
